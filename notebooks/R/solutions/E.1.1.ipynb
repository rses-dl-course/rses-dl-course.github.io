        "# Create model\n",
        "model <- keras_model_sequential() %>%\n",
        "    layer_dense(units = 1, input_shape = 1)\n",
        "\n",
        "# Compile model\n",
        "model %>%\n",
        "    compile(optimizer = optimizer_adam(0.1), \n",
        "    loss='mean_squared_error')\n",
        "\n",
        "# Fit model\n",
        "history <- model %>% fit(\n",
        "    x = x_set, \n",
        "    y = y_set,\n",
        "    epochs = 500,\n",
        ")"
      ],
